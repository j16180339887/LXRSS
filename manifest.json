{
    "name": "LXRSS",
    "description": "Lightweight RSS Feed Reader for Chrome",
    "version": "1.0",
    "manifest_version": 2,
    "chrome_url_overrides": {
        "newtab": "tab.html"
    },
    "browser_action": {
        "default_icon": {
            "48": "img/icon48.png",
            "128": "img/icon128.png"
        },
        "default_title": "LXRSS"
    },
    "icons": {
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "incognito": "split",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "tabs",
        "storage",
        "<all_urls>"
    ]
}